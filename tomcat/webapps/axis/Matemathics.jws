import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

import org.midas.ms.trader.webservices.Adapter;

public class Matemathics
{
	public List sum(java.lang.Integer num01, java.lang.Integer num02) throws Exception 
	{
		Map  in = new HashMap();
		List out = new ArrayList(10);

		in.put("num01",num01);
		in.put("num02",num02);

		Adapter.provide("Calculator","add",in,out);

		return out;
	}
}